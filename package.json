{
  "name": "semgrep-snippets",
  "displayName": "Semgrep Snippets",
  "description": "Semgrep Snippets",
  "version": "0.0.5",
  "publisher": "lewisardern",
  "icon": "semgrep-icon.png",
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "yaml",
        "path": "./snippets/semgrep.json"
      },
      {
        "language": "java",
        "path": "./snippets/java.json"
      },
      {
        "language": "go",
        "path": "./snippets/go.json"
      }
    ]
  },
  "scripts": {
    "package": "npx vsce package",
    "publish": "npx vsce publish"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LewisArdern/semgrep-snippets.git"
  },
  "keywords": [
    "semgrep"
  ],
  "engines": {
    "vscode": "^1.40.0"
  },
  "author": "Lewis Ardern",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LewisArdern/semgrep-snippets/issues"
  },
  "homepage": "https://github.com/LewisArdern/semgrep-snippets#readme",
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@vscode/test-web": "0.0.22",
    "typescript": "^4.6.2",
    "vscode": "^1.1.37"
  }
}
